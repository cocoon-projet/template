{
    "name": "cocoon-projet/template",
    "type": "library",
    "description": "Un moteur de template pour le language php",
    "keywords": ["templating", "template", "view", "views" ],
    "homepage": "https://github.com/cocoon-projet/template",
    "license": "MIT",
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^9",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Cocoon\\": "src/Cocoon/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php":"^8.0",
        "nesbot/carbon": "^2"
    },
    "scripts": {
        "test": ["vendor/bin/phpcbf",
                "vendor/bin/phpcs",
                "vendor/bin/phpunit"]
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
